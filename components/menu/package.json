{
  "name": "@cagov/ds-dropdown-menu",
  "version": "1.0.14",
  "description": "",
  "main": "src/index.js",
  "type": "module",
  "customElements": "custom-elements.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/cagov/design-system/tree/main/components/menu"
  },
  "scripts": {
    "build": "sass src/index.scss index.css",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.js\" --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --ext .js,.html . --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --ext .js,.html . --ignore-path .gitignore --fix",
    "lint:prettier": "prettier \"**/*.js\" --check --ignore-path .gitignore",
    "prepare": "cd ../../ && husky install components/menu/.husky",
    "prepublishOnly": "npm run format && npm run lint && npm run build && npm run test",
    "test": "web-test-runner \"test/**/*.js\" --node-resolve",
    "test:visual": "web-test-runner \"test/**/*.js\" --node-resolve --config test-config.js"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@open-wc/eslint-config": "^4.3.0",
    "@open-wc/testing": "^3.0.0-next.2",
    "@web/test-runner": "^0.13.16",
    "@web/test-runner-puppeteer": "^0.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-lit": "^1.5.1",
    "husky": "^7.0.2",
    "prettier": "^2.3.2",
    "sass": "^1.38.1"
  }
